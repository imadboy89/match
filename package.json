{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "deploy": "gh-pages -d web-build",
    "testd": "powershell -File generate_updates_info.ps1",
    "predeploy": "powershell -File generate_updates_info.ps1 && npx expo export:web",
    "depl": "gh-pages -d web-build",
    "publish": "expo publish",
    "pubdep": "expo publish && npm run predeploy",
    "start_wp": "webpack-dev-server -d --config ./web/webpack.config.js --inline --hot --colors",
    "start": "npx expo start --tunnel",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start:web",
    "eject": "expo eject",
    "doall": "powershell -File generate_updates_info.ps1 && git add . && git commit -a -m \"fix bugs and more ..DOALL\" && git push origin main  && eas update && npm run deploy",
    "git": "git commit -a -m \"fix bugs and more ..DOALL\" && git push origin",
    "eas-build-pre-install": "npm config set legacy-peer-deps true"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "2.0.0",
    "@react-native-community/datetimepicker": "8.2.0",
